{
  "name": "yp-uni",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "registry": "https://packages.aliyun.com/6077b956afdac3288fbb56d1/npm/npm-registry/",
    "access": "public"
  },
  "scripts": {
    "lint:eslint": "eslint . --ext .js --ext .ts --ext .tsx --ext .vue --no-fix --cache",
    "lint:eslint:fix": "eslint . --ext .js --ext .ts --ext .tsx --ext .vue --fix",
    "lint:format": "prettier -c ./ --cache .",
    "lint:format:fix": "prettier -w ./",
    "lint:style": "stylelint **/*.{css,less,scss} --cache",
    "lint:style:fix": "stylelint  **/*.{css,less,scss} --fix",
    "dev:mp-weixin": "uni -p mp-weixin",
    "prepare": "husky install",
    "build:lib": "npm run dts && cross-env MODE=NPM node ./scripts/build/index.js",
    "dts": "rimraf dts && vue-tsc --declaration --emitDeclarationOnly -p ./tsconfig.declaration.json",
    "type-check": "vue-tsc --noEmit",
    "ci:version": "standard-version",
    "ci:publish": "pnpm run build:lib && cd dist/npm && npm publish",
    "ci:tag": "node scripts/pushTag.js",
    "ci:lib": "pnpm ci:version && pnpm run build:lib && cd dist/npm && npm publish && git push",
    "icon:sync": "node scripts/syncIcon.mjs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,json}": [
      "eslint --fix",
      "prettier --write",
      "bash -c 'npm run type-check'"
    ],
    "*.{scss,css,less}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
